	"scripts": {
		"build": "next build",
		"build:cap": "CAPACITOR_BUILD=true next build && CAPACITOR_BUILD=true next export && npx cap sync",
		"build:image": "docker build -t ygktool .",
		"db:init": "prisma generate",
		"db:update": "prisma db push",
		"db:panel": "prisma studio",
		"dev": "next",
		"dev:cap": "CAPACITOR_BUILD=true next",
		"i18n:update": "node scripts/i18n-loader.js",
		"local-build": "next build",
		"start": "next start -p 4000",
		"start:container": "docker run -p 4000:4000 ygktool",
		"test": "jest ./__tests__ --testPathIgnorePatterns support/*",
		"test:unit": "jest --verbose"
	},
